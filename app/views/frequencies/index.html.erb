<p id="notice"><%= notice %></p>

<h1>Frequencias Cadastradas</h1>
<%= link_to 'novo registro', new_frequency_path, class: 'btn btn-primary pull-right' %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Person</th>
      <th>Frequency date</th>
      <th>Time input</th>
      <th>Time output</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @frequencies.each do |frequency| %>
      <tr>
        <td><%= frequency.person_id %></td>
        <td><%= frequency.frequency_date %></td>
        <td><%= frequency.time_input %></td>
        <td><%= frequency.time_output %></td>
        <td><%= link_to 'exibir', frequency %></td>
        <td><%= link_to 'editar', edit_frequency_path(frequency) %></td>
        <td><%#= link_to 'deletar', frequency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

