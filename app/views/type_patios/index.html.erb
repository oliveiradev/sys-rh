<p id="notice"><%= notice %></p>

<h1>Listing Type Patios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Initial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_patios.each do |type_patio| %>
      <tr>
        <td><%= type_patio.name %></td>
        <td><%= type_patio.initial %></td>
        <td><%= link_to 'Show', type_patio %></td>
        <td><%= link_to 'Edit', edit_type_patio_path(type_patio) %></td>
        <td><%= link_to 'Destroy', type_patio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type patio', new_type_patio_path %>
